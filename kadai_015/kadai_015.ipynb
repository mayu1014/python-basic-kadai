        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(self.name, self.age)\n",
        "\n",
        "# Humanクラスのインスタンスを作成し、変数に代入\n",
        "person = Human(\"太郎\", 25)\n",
        "\n",
        "# 情報を出力\n",
        "person.printinfo()\n"
      ]
    }
